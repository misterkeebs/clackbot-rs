{
  "manifest_version": 2,
  "name": "MonkeyType WPM Monitor",
  "version": "1.0",
  "description": "Monitors WPM changes on monkeytype.com and logs them.",
  "permissions": [
    "activeTab",
    "https://clackbot-rs-production.up.railway.app/"
  ],
  "content_scripts": [
    {
      "matches": ["*://monkeytype.com/*"],
      "js": ["inject.js"]
    }
  ]
}

